{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "globals": {
    "React": "readonly",
    "JSX": "readonly"
  },
  "rules": {
    "no-unused-vars": 1,
    "quotes": [2, "single"],
    "quote-props": [2, "as-needed"],
    "brace-style": [2, "1tbs", { "allowSingleLine": false }]
  }
}
